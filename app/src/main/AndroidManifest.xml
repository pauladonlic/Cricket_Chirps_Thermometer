<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cricketchirpsthermometer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cricket2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cricket2"
        android:supportsRtl="true"
        android:theme="@style/Theme.CricketChirpsThermometer">
        <activity android:name=".CalculateTemperature"></activity>
        <activity android:name=".FinalTemperature" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>